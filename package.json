{
  "name": "LXHistory_Sync",
  "displayName": "LXHistory_Sync",
  "version": "1.0.0",
  "description": "LXHistory_Sync - WebDAV 历史记录同步扩展",
  "packageManager": "npm@10.0.0",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.10.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-virtuoso": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.8.1",
    "typescript": "5.3.3",
    "typescript-eslint": "^8.55.0"
  },
  "manifest": {
    "permissions": [
      "history",
      "storage"
    ],
    "action": {
      "default_title": "LXHistory_Sync",
      "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "background": {
      "service_worker": "background.ts"
    }
  },
  "plasmo": {
    "assets": [
      "assets/icon.png"
    ],
    "manifest": {
      "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
      },
      "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
}
